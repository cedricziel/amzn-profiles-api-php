{
    "name": "cedricziel/amznprofilesapiphp",
    "description": "An API client for Amazon Advertising profiles",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CedricZiel\\AmznAdvertisingProfilesApi\\": "src/",
            "CedricZiel\\AmznAdvertisingProfilesApi\\Generated\\": "generated/"
        }
    },
    "authors": [
        {
            "name": "cedricziel",
            "email": "mail@example.com"
        }
    ],
    "require-dev": {
        "jane-php/open-api-3": "^7.8",
        "friendsofphp/php-cs-fixer": "^3.59",
        "symfony/yaml": "^7.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.3",
        "jane-php/open-api-runtime": "^7.8"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
